{
    "name": "opendatabio/opendatabio",
    "description": "OpenDataBio.",
    "license": "GPLv3",
    "type": "project",
    "require": {
        "php": "^7.0",
        "andrechalom/laravel-multiselect": "@dev",
        "barryvdh/laravel-translation-manager": "^0.5",
        "baum/baum": "2.x-dev",
        "fico7489/laravel-pivot": "^3.0",
        "fx3costa/laravelchartjs": "^2.7",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "renanbr/bibtex-parser": "^2.1",
        "ryakad/pandoc-php": "^1.0",
        "sopamo/laravel-filepond": "^0.5.0",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/simple-excel": "^1.7",
        "stichoza/google-translate-php": "^4.0",
        "tanmuhittin/laravel-google-translate": "^1.0",
        "wyndow/fuzzywuzzy": "^0.6.0",
        "yajra/laravel-datatables-buttons": "^4.9",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "composer/composer": "^1.10",
        "doctrine/annotations": "^1.4",
        "doctrine/instantiator": "^1.3",
        "doctrine/dbal": "^2.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "maximebf/debugbar": "^1.16",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "fixme": [
            "./vendor/bin/phpunit",
            "./vendor/bin/php-cs-fixer fix --config=.php_cs --verbose"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "../andrechalom/laravel-multiselect",
            "options": {
                "symlink": true
            }
        }
    }
}
